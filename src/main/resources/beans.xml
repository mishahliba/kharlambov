<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id = "commadelimiter" class="com.di.entity.CommaDelimiter"></bean>
    <bean id="colondelimiter" class="com.di.entity.ColonDelimiter"></bean>
    <bean id = "delimiterService" class="com.di.service.DelimiterService" >
        <property name="delimiter">
            <ref bean="colondelimiter"></ref>
        </property>
    </bean>
</beans>